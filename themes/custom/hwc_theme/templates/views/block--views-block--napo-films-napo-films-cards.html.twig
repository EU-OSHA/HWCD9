
<h3 class="h4 mb-4">{{ content['#title']['#markup']}}</h3>
<a href="{{ file_url('http://hwc.ddev.site/sites/hwc/files/styles/large/public/hwc/news/eu_diversity_month_220.jpg') }}">
  Download all
</a>

<div class="">
  {% set results = content['#view'].result %}
  {% for index, item in results %}
  {% set item = item._entity %}

    <article class="card" >

{#      <img class="card-img-top"  alt="Card image cap">#}
      <div class="card-img-top">
        {{ item.field_youtube.0.entity.field_media_oembed_video|view }}
      </div>
      <div class="card-body">
        <div class="share-networks">
          <ul class="napo-actions">
            <li class="share-link fas fa-share-alt"><label class="sr-only">Share</label></li>
            <li class="download-film fas fa-download">
              <a href="{{file_url('item.field_video.0.entity.uri.value')}}">
                <i class="fas fa-download"></i>
              </a>
            </li>
          </ul>
          <ul class="list-networks d-none">
            <li class="close-share fas fa-times"></li>
            <li class="fb">
              <a href="https://www.facebook.com/sharer/sharer.php?u=www.google.com" onclick="javascript:window.open(this.href, '', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=300,width=600');return false;"
                 target="_blank"><span>facebook</span></a>
            </li>
            <li class="tw">
              <a href="https://twitter.com/share?url=URLENCODED_URL&via=TWITTER_HANDLE&text=TEXT"
                 onclick="javascript:window.open(this.href, '', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=300,width=600');return false;"
                 target="_blank"><span>twitter</span></a> </li>
            <li class="ln">
              <a href="https://www.linkedin.com/shareArticle?mini=true&url=YOUR_URL_HERE" onclick="javascript:window.open(this.href, '', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=300,width=600');return false;"
                 target="_blank"><span>linkedin</span></a> </li>
          </ul>
        </div>
        <h3 class="card-title">{{ item.title.value }}</h3>
        <p class="card-text">{{ item.body.value | striptags}}</p>
        <p>Download file: <a href="{{ file_url(item.field_video.0.entity.uri.value) }}"> {{item.field_video.0.entity.uri.value}} </a> </p>


      </div>
    </article>
  {% endfor %}


